{
  "packages": [
    "nginx@latest",
    "php@7.4.29",
    "php74Packages.composer@latest",
    "php74Extensions.bcmath@latest",
    "php74Extensions.gd@latest",
    "php74Extensions.gettext@latest",
    "php74Extensions.intl@latest",
    "php74Extensions.pcntl@latest",
    "php74Extensions.shmop@latest",
    "php74Extensions.soap@latest",
    "php74Extensions.sockets@latest",
    "php74Extensions.sysvsem@latest",
    "php74Extensions.xmlrpc@latest",
    "php74Extensions.zip@latest",
    "php74Extensions.mbstring@latest",
    "php74Extensions.curl@latest",
    "php74Extensions.opcache@latest",
    "php74Extensions.redis@latest",
    "php74Extensions.pdo@latest",
    "php74Extensions.pdo_mysql@latest",
    "nodejs@22.2.0",
    "nodePackages.pm2@latest",
    "nodePackages.pnpm@8.15.5",
    "nodePackages.yarn@1.22.22"
  ],
  "env": {
    "PROJECT_DIR": "$PWD",
    "PHPRC": "$PWD/config/php/php.ini",
    "PHPFPM": "$PWD/config/php/php-fpm.conf",
    "PHPFPM_PORT": "8089",
    "PHPFPM_USER": "cheems",
    "PHPFPM_GROUP": "cheems",
    "PHPFPM_PID_FILE": "$PWD/.devbox/virtenv/php/php-fpm.pid",
    "PHPFPM_ERROR_LOG_FILE": "$PWD/.devbox/virtenv/php/php-fpm.log",
    "NGINX_CONFDIR": "$PWD/config/nginx",
    "NGINX_PATH_PREFIX": "$PWD/.devbox/virtenv/nginx",
    "NGINX_PIDDIR": "$PWD/.devbox/virtenv/nginx/nginx.pid",
    "NGINX_TMPDIR": "$PWD/.devbox/virtenv/nginx/temp",
    "NGINX_USER": "cheems",
    "NGINX_GROUP": "cheems",
    "DEVBOX_COREPACK_ENABLED": "true"
  },
  "shell": {
    "init_hook": [],
    "scripts": {}
  }
}
